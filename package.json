{
  "name": "vendure-angular-storefront",
  "version": "0.1.24",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201",
    "build": "ng build",
    "analyze-prod-bundle": "ng build --prod --statsJson=true && webpack-bundle-analyzer dist/browser/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate-types": "gql-gen --config codegen.yml",
    "dev:ssr": "ng run reference-storefront:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run reference-storefront:server:production",
    "build:ssr:docker": "ng build --configuration=production,docker && ng run reference-storefront:server:docker",
    "prerender": "ng run reference-storefront:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/forms": "^11.2.14",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/platform-server": "^11.2.14",
    "@angular/router": "^11.2.14",
    "@angular/service-worker": "^11.2.14",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.31",
    "@fortawesome/free-brands-svg-icons": "^5.15.0",
    "@fortawesome/free-solid-svg-icons": "^5.15.0",
    "@nguniversal/express-engine": "^11.2.1",
    "apollo-angular": "^2.0.4",
    "bootstrap": "^4.5.2",
    "compression": "^1.7.4",
    "core-js": "^3.6.5",
    "express": "^4.15.2",
    "graphql": "^15.0.0",
    "photoswipe": "^4.1.3",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4",
    "@apollo/client": "^3.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.13",
    "@angular/cli": "^11.2.13",
    "@angular/compiler-cli": "^11.2.14",
    "@angular/language-service": "^11.2.14",
    "@nguniversal/builders": "^11.2.1",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.8",
    "@types/graphql": "^14.0.5",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^14.11.2",
    "@types/photoswipe": "^4.1.0",
    "codelyzer": "^6.0.0",
    "@graphql-codegen/add": "^2.0.1",
    "@graphql-codegen/cli": "^1.13.2",
    "@graphql-codegen/fragment-matcher": "^1.17.8",
    "@graphql-codegen/typescript": "^1.13.2",
    "@graphql-codegen/typescript-compatibility": "^2.0.0",
    "@graphql-codegen/typescript-operations": "^1.13.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
